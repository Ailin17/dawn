{
  "type": "header",
  "name": "Header",
  "sections": {
    "header_section": {
      "type": "header",
      "blocks": {
        "header-logo": {
          "type": "_header-logo",
          "static": true,
          "settings": {
            "custom_height": 20,
            "custom_height_mobile": 20,
            "padding-block-start": 16,
            "padding-block-end": 16,
            "padding-inline-start": 10,
            "padding-inline-end": 10
          }
        },
        "header-menu": {
          "type": "_header-menu",
          "static": true,
          "settings": {
            "menu": "main-menu",
            "menu_style": "featured_products"
          }
        }
      },
      "settings": {
        "logo_position": "center",
        "menu_position": "left",
        "menu_row": "top",
        "search_style": "modal",
        "search_position": "right",
        "search_row": "top",
        "show_country": true,
        "country_selector_style": true,
        "localization_position": "right",
        "localization_row": "top",
        "section_width": "page-width",
        "color_scheme_top": "scheme-2"
      }
    }
  },
  "order": ["header_section"]
}
